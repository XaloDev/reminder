er-dom';\r\nimport firebase from '../../config/firebase';\r\n\r\nimport './evento-card.css';\r\n\r\nfunction EventoCard({id, img, titulo, detalhes, visualizacoes}){\r\n    \r\n    const [urlImagem, setUrlImagem] = useState();\r\n\r\n    useEffect(() => {\r\n        firebase.storage().ref(`imagens/${img}`).getDownloadURL().then(url => setUrlImagem(url));\r\n    }, [urlImagem]);\r\n\r\n    return(\r\n        <div className=\"cold-md-3 col-sm-4\">\r\n            <img src={urlImagem} className=\"card-img-top img-cartao\" alt=\"Imagem do Evento\" />\r\n            <div className=\"card-body\">\r\n                <h5>{titulo}</h5>\r\n                <p className=\"card-text text-justify\">\r\n                    {detalhes}                  \r\n                </p>\r\n\r\n                <div className=\"row rodape-card d-flex align-items-center\">\r\n                    <div className=\"col-6\">\r\n                        <Link to={'/eventodetalhes/' + id} className=\"btn btn-sm btn-detalhes\">+ detalhes</Link>\r\n                    </div>\r\n\r\n                    <div className=\"col-6 text-right\">\r\n                        <i class=\"fas fa-eye\"></i> <span>{visualizacoes}</span>\r\n                    </div>\r\n                </div>\r\n            </div>\r\n        </div>\r\n    )\r\n}\r\n\r\nexport default EventoCard;\r\n","C:\\Users\\icaro\\Desktop\\Eu\\eventos\\eventos\\src\\view\\evento-detalhes\\index.js",["193"],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\index.js",[],["194","195"],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\App.js",[],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\store\\index.js",[],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\view\\login\\index.js",[],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\view\\usuario-novo\\index.js",[],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\view\\home\\index.js",[],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\view\\usuario-recuperar-senh