ventoDetalhes(props){\r\n\r\n    const [evento, setEvento] = useState({});\r\n    const [urlImg, setUrlImg] = useState();\r\n    const usuarioLogado = useSelector(state => state.usuarioEmail);\r\n    const [carregando, setCarregando] = useState(1);\r\n\r\n    useEffect(() =>{\r\n        if(carregando){\r\n\r\n        \r\n        firebase.firestore().collection('eventos').doc(props.match.params.id).get().then(resultado => {\r\n            setEvento(resultado.data())\r\n            firebase.firestore().collection('eventos').doc(props.match.params.id).update('visualizacoes', resultado.data().visualizacoes + 1)\r\n            firebase.storage().ref(`imagens/${resultado.data().foto}`).getDownloadURL().then(url => {\r\n                setUrlImg(url)\r\n                setCarregando(0);\r\n            });\r\n        });\r\n    }else{\r\n        firebase.storage().ref(`imagens/${evento.foto}`).getDownloadURL().then(url => setUrlImg(url))\r\n     }\r\n},[])\r\n\r\n    return(\r\n        <>\r\n        <Navbar/>\r\n        <div className=\"container-fluid\">\r\n            {\r\n\r\n            carregando ? <div className=\"row mt-5\"><div class=\"spinner-border text-danger mx-auto\" role=\"status\"><span class=\"sr-only\"></span></div></div>\r\n            :\r\n            \r\n            <div>\r\n            <div className=\"row\">\r\n                <img src={urlImg} className=\"img-banner\" alt=\"Banner\"/>\r\n                <div className=\"col-12 text-right mt-1 visualizacoes\">\r\n                <i class=\"fas fa-eye\"></i> <span>{evento.visualizacoes + 1}</span>\r\n                \r\n                </div>\r\n                <h3 className=\"mx-auto mt-5 titulo\"><strong>{evento.titulo}</strong></h3>\r\n            </div>\r\n            \r\n\r\n            <div className=\"row mt-5 d-flex justify-content-around\">\r\n                <div className=\"col-md-3 col-sm-12 box-info p-3 my-2\">\r\n                    <i className=\"fas fa-ticket-alt fa-2x\"></i>\r\n                    <h5><strong>Tipo</strong></h5>\r\n                    <span className=\"mt-3\">{evento.tipo}</span>\r\n                </div>\r\n\r\n                <div className=\"col-md-3 col-sm-12 box-info p-3 my-2\">\r\n                    <i className=\"fas fa-calendar-alt fa-2x\"></i>\r\n                    <h5><strong>Data</strong></h5>\r\n                    <span className=\"mt-3\">{evento.data}</span>\r\n                </div>\r\n\r\n                <div className=\"col-md-3 col-sm-12 box-info p-3 my-2\">\r\n                    <i className=\"fas fa-clock fa-2x\"></i>\r\n                    <h5><strong>Hora</strong></h5>\r\n                    <span className=\"mt-3\">{evento.hora}</span>\r\n                </div>\r\n            </div>\r\n            <div className=\"row box-detalhes mt-5\">\r\n                <div className=\"col-12 text-center\">\r\n                <h5><strong>Detalhes do Evento</strong></h5>\r\n                </div>\r\n                \r\n                <div className=\"col-12 text-center\">\r\n                <p>{evento.detalhes}</p>\r\n                </div>\r\n            </div>\r\n\r\n            {\r\n                usuarioLogado === evento.usuario ?\r\n                <Link to=''className=\"btn-editar\"><i className=\"fas fa-pen-square fa-3x\"></i></Link>\r\n                : ''\r\n            }\r\n            </div>\r\n            }\r\n        </div>\r\n        </>\r\n    )\r\n}\r\n\r\nexport default EventoDetalhes;","C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\store\\usuarioReducer.js",[],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\config\\firebase.js",[],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\components\\navbar\\index.js",[],"C:\\Users\\icaro\\Desktop\\eu\\eventos\\eventos\\src\\components\\evento-card\\index.js",["179"],"C:\\Users\\icaro\\Desktop\\eu\\reminder\\reminder\\src\\index.js",[],"C:\\Users\\icaro\\Desktop\\eu\\reminder\\reminder\\src\\App.js",[],"C:\\Users\\icaro\\Desktop\\eu\\remi